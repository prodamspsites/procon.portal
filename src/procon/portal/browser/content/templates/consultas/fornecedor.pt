<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">
<body>
  <div id="content-core" metal:fill-slot="main">
  <!--RECLAMACOES-->
  <div class="wrapAdmin">
    <div class="divReclamacoes">
      <h1>Fornecedores cadastrados</h1>
      <!-- <span class="total">Total: <span tal:define="perguntas view/buscarPerguntaResposta;" tal:replace="python:perguntas['total']"></span></span> -->

      <div class="filtrarPor"><strong>Filtrar por:</strong>
      <form tal:attributes="action string:${context/portal_url}/buscar_fornecedores" method="GET" name="filtro">
        <select class="selectFilter"><option>Nome</option><select>
        <input type="text"  name="filtro" placeholder="Digite um valor para filtrar" class="inputFilter"/>
        <input type="submit" value="FILTRAR" class="btnFiltrar" />
      </form>
    </div>
      <div class="tableReclamacoes">
      <table id="table_id_reclamacoes" class="display">
          <thead>
            <tr>
              <th width="1%" style="display:none">
                <span>.</span>
              </th>
              <th width="17%">
                <span>Data</span>
              </th>
              <th width="40%">
                <span>Razão Social</span>
              </th>
              <th width="23%">
                <span>CNPJ</span>
              </th>
              <th width="20%">
                <span>SINDEC</span>
              </th>
            </tr>
          </thead>
          <tbody>
            <tr tal:condition="dados" tal:define="dados view/buscaFornecedor" tal:repeat="pergunta dados">
              <td tal:attributes="rel python:dados[int(repeat.pergunta.number) -1][-1]" class="reclamacao_buscar _id">07/06/2016</td>
              <td tal:attributes="class python:'categoria'"
                  tal:content="python:dados[int(repeat.pergunta.number) -1][1]">Cadastro</td>
              <td tal:attributes="class python:'pergunta'"
                  tal:content="python:dados[int(repeat.pergunta.number) -1][2]">00998821888</td>
              <th tal:define="lido python:dados[int(repeat.pergunta.number) -1][-3]; input string:<input class='lido_reclamacoes' type='checkbox' value='cadastro' />; disable string:<input class='lido' disabled=disabled' checked type='checkbox' value='cadastro' />" tal:content="structure python:lido and disable+' <font color=gray>Cadastrado</font>' or input " style="background-color:white"></th>
                <div class="detalhesDuvida" style="display:none" tal:define="labels
                  python:['Razão Social',
                          'CNPJ',
                          'Nome fantasia',
                          'CNAE Fiscal Principal',
                          'Segmento de mercado',
                          'Nome completo do responsável',
                          'CEP',
                          'Endereço',
                          'Número',
                          'Complemento',
                          'Bairro',
                          'UF',
                          'Cidade',
                          'Telefone',
                          'E-mail',
                          'Arquivos anexados'
                         ]" tal:repeat="label labels">

                    <div class="detalhesDuvida" tal:condition="python: not repeat.label.end">
                      <strong class="tituloConsumidor" tal:content="label">Pergunta:</strong>
                      <div class="respostaConsumidor">
                        <span tal:define="count python:int(repeat.label.number);" tal:content="python:dados[int(repeat.pergunta.number) -1][count]"></span>
                      </div>
                    </div>

                    <div class="detalhesDuvida"
                         tal:condition="repeat/label/end">
                      <strong class="tituloConsumidor" tal:content="label">Pergunta:</strong>
                          <div class="respostaConsumidor">
                            <a href="#" class="fileDownload"
                              tal:define="count python:int(repeat.label.number);"
                              tal:condition="python:dados[int(repeat.pergunta.number) -1][count +1] != 'x'"
                              tal:content="python:dados[int(repeat.pergunta.number) -1][count +1]"
                              tal:attributes="data-binary python:dados[int(repeat.pergunta.number) -1][count]; data-mimetype python:dados[int(repeat.pergunta.number) -1][count +2]"></a>
                          </div>
                    </div>

              </div>
            </tr>
          </tbody>
          <!-- bower:js -->
          <!-- endbower -->
      </table>
      </div>

      <tal:result tal:condition="perguntas" tal:define="perguntas view/buscaFornecedor">
      <div class="paginationReclamacoes">
      </div>
      </tal:result>
    </div>
  </div>
  <!--RECLAMACOES-->
  <div class="teste" style="display:none;">
    <div class="tableReclamacoes">
    <table>
      <tr>
        <th width="12%">
          <span>Data</span>
          <a href="#" title="ordenar mais recente" class="setaDown active"></a>
          <a href="#" title="ordenar mais recente" class="setaUp"></a>
        </th>
        <th width="13%">
          <span>Categoria</span>
          <a href="#" title="ordenar mais recente" class="setaDown active"></a>
          <a href="#" title="ordenar mais recente" class="setaUp"></a>
        </th>
        <th width="25%">
          <span>Nome</span>
          <a href="#" title="ordenar mais recente" class="setaDown active"></a>
          <a href="#" title="ordenar mais recente" class="setaUp active"></a>
        </th>
      </tr>
        <tr>
          <td id="tbl1"></td>
          <td id="tbl2"></td>
          <td id="tbl3"></td>
        </tr>
    </table>
    </div>
  </div>


  </div>

</body>
</html>
